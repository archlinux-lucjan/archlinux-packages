pkgname=lucjan-keyring
pkgver=20171014
pkgrel=1
pkgdesc="Lucjan's PGP keyring"
arch=('any')
license=('GPL')
install="${pkgname}.install"
source=("httpss://github.com/archlinux-lucjan/${pkgname}/archive/v${pkgver}.tar.gz")
sha256sums=('5a8c581b8e1c8842287280f4fb59bb5556d6f21e3c228fc9f8cdd242ed1d6090')


package() {
	cd "${srcdir}/${pkgname}-${pkgver}"
	make PREFIX=/usr DESTDIR=${pkgdir} install
} 
